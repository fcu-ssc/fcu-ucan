{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bulma@0.9.3",
      "destination": "wwwroot/lib/bulma/",
      "files": [
        "css/bulma.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bulma-social@2.0.0",
      "destination": "wwwroot/lib/bulma-social/",
      "files": [
        "css/all.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bulma-carousel@4.0.24",
      "destination": "wwwroot/lib/bulma-carousel/",
      "files": [
        "dist/css/bulma-carousel.min.css",
        "dist/js/bulma-carousel.min.js"
      ]
    },
    {
      "library": "aos@2.3.4",
      "destination": "wwwroot/lib/aos/",
      "files": [
        "aos.js",
        "aos.css"
      ]
    },
    {
      "library": "font-awesome@5.15.4",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "js/all.min.js"
      ]
    }
  ]
}