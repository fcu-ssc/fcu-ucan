{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bulma@0.9.3",
      "destination": "wwwroot/lib/bulma/",
      "files": [
        "css/bulma.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bulma-social@2.0.0",
      "destination": "wwwroot/lib/bulma-social/",
      "files": [
        "css/all.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bulma-carousel@4.0.24",
      "destination": "wwwroot/lib/bulma-carousel/",
      "files": [
        "dist/css/bulma-carousel.min.css",
        "dist/js/bulma-carousel.min.js"
      ]
    },
    {
      "library": "aos@2.3.4",
      "destination": "wwwroot/lib/aos/",
      "files": [
        "aos.js",
        "aos.css"
      ]
    },
    {
      "library": "font-awesome@5.15.4",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "js/all.min.js"
      ]
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "jquery-validate@1.17.0",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bulma-responsive-tables@1.2.3",
      "destination": "wwwroot/lib/bulma-responsive-tables/",
      "files": [
        "css/main.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bulma-switch@2.0.0",
      "destination": "wwwroot/lib/bulma-switch/",
      "files": [
        "dist/css/bulma-switch.min.css"
      ]
    }
  ]
}